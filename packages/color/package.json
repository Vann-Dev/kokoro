{
  "name": "@kokoro-ui/color",
  "version": "0.0.7",
  "description": "Kokoro UI color tailwind plugin",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "keywords": [
    "theme",
    "kokoro-ui",
    "ui",
    "ux",
    "component",
    "tailwindcss",
    "tailwindcss",
    "nextjs"
  ],
  "files": [
    "./dist",
    "./package.json"
  ],
  "engines": {
    "node": ">=20.0.0"
  },
  "devDependencies": {
    "@hazmi35/eslint-config": "^11.0.0",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "typescript": "^5.3.3"
  },
  "peerDependencies": {
    "typescript": "*",
    "tailwindcss": "*"
  },
  "eslintConfig": {
    "parser": "@typescript-eslint/parser",
    "plugins": [
      "@typescript-eslint"
    ],
    "parserOptions": {
      "project": "./tsconfig.json"
    },
    "extends": [
      "eslint:recommended",
      "plugin:@typescript-eslint/recommended",
      "@hazmi35/eslint-config/typescript"
    ],
    "rules": {
      "class-methods-use-this": "off"
    }
  },
  "scripts": {
    "lint": "eslint src --ext ts",
    "lint:fix": "eslint src --fix --ext ts",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vann-Dev/Kokoro.git",
    "directory": "packages/color"
  },
  "author": "Vann-Dev <me@vann.my.id> (https://vann.my.id)",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/Vann-Dev/Kokoro/issues"
  },
  "homepage": "https://github.com/Vann-Dev/Kokoro#readme"
}