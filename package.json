{
  "name": "sage-ui",
  "version": "1.0.0",
  "type": "module",
  "description": "Sage UI is a package used for component using tailwindcss",
  "main": "dist",
  "scripts": {
    "build": "turbo run build",
    "lint": "turbo run lint",
    "lint:fix": "turbo run lint:fix",
    "docs:start": "turbo run start --filter=@sage-ui/storybook",
    "docs:dev": "turbo run dev --filter=@sage-ui/storybook",
    "docs:build": "turbo run build --filter=@sage-ui/storybook",
    "component:build": "turbo run build --filter=!@sage-ui/storybook"
  },
  "keywords": [],
  "author": "Vann-Dev",
  "license": "GPL-3.0",
  "devDependencies": {
    "@storybook/react": "^7.6.6",
    "turbo": "^1.11.2"
  },
  "dependencies": {
    "lerna": "^8.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Vann-Dev/sage-ui.git"
  },
  "bugs": {
    "url": "https://github.com/Vann-Dev/sage-ui/issues"
  },
  "homepage": "https://github.com/Vann-Dev/sage-ui#readme"
}
