{
    "$schema": "https://turborepo.org/schema.json",
    "pipeline": {
        "build": {
            "dependsOn": [
                "^build"
            ]
        },
        "lint": {
            "dependsOn": [
                "^lint"
            ]
        },
        "dev": {
            "dependsOn": [
                "^dev"
            ]
        },
        "start": {
            "dependsOn": [
                "^start"
            ]
        },
        "lint:fix": {
            "dependsOn": [
                "^lint"
            ]
        }
    }
}